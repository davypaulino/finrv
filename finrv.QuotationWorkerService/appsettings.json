{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Broker": {
    "Host": "localhost:9094",
    "GroupId": "quotation-update-workers",
    "Topics": {
      "QuotationUpdateListener": "finrv-quotations-updates"
    }
  },
  "RetryPolicy": {
    "RetryCount": 5,
    "SleepDurationPower": 2
  }
}
